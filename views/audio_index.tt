<div id='audio'>
[% FOREACH file IN files %]
	<div class='audio'>
		<h4><a style="text-decoration: none;" href="/audio/[% file.slug %]">[% file.title %]</a></h4>
		[% file.html %]
		<p class='audio-sig'><i>posted at [% file.updated %] by [% file.who %]</i></p>
	</div>
[% END %]
</div>
<div style="clear:both"></div>
